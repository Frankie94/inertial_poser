<?xml version="1.0"?>
<launch>
    
    <node pkg="inertial_poser" name="pose_optimizer" type="poser" args="" output="screen">
        <param name="useRotation" type="bool" value="true"/>
        <param name="usePosePrior" type="bool" value="false"/>
        <param name="useFilter" type="bool" value="false"/>
        <param name="useAcceleration" type="bool" value="true"/>
        <param name="useConstraint" type="bool" value="true"/>
        <param name="PoseProjectionWeight" type="double" value="0.001"/>
        <param name="PoseDeviationWeight" type="double" value="0.001"/>
        <param name="OrientationWeight" type="double" value="1"/>
        <param name="AccelerationWeight" type="double" value="0.001"/>
        <param name="PositionWeight" type="double" value="0.001"/>
    </node>
</launch>
